{
    "manifest_version": 3,
    "name": "Gimkit Autosolver",
    "author": "Alex L",
    "description": "Show / Highlight correct answers on Gimkit.",
    "version": "1.0",
    "action": {
        "default_icon": "static/icons/icon.png"
    },
    "content_scripts": [
        {
            "world": "MAIN",
            "js": ["index.js"],
            "run_at": "document_start",
            "matches": ["https://www.gimkit.com/join*"]
        }
    ],
    "icons": {
        "128": "static/icons/icon.png"
    }
}
